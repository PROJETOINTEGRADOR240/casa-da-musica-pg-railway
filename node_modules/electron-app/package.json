{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "mac": "electron-packager . --platform=darwin --arch=all --overwrite",
    "package": "asar pack eca.app/Contents/Resources/app electron-quick-start.app/Contents/Resources/app.asar",
    "sign":"node codesign.js",
    "pack": "build",
    "dist": "build"
  },
  "build": {
    "app-bundle-id": "womp.id",
    "app-category-type": "category.type",
    "iconUrl": "build/icon.icns"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "asar": "^0.6.1",
    "electron-packager": "*",
    "electron-prebuilt": "*"
  },
  "dependencies": {
    "async": "^1.5.2",
    "fs-extra": "^0.26.6",
    "eca-updater": "*",
    "electron-updater": "*"
  }
}
